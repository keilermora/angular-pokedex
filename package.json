{
  "name": "pokedex-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test --code-coverage --browsers=Chrome",
    "test:cov": "ng test --code-coverage --no-watch --no-progress --browsers=ChromeHeadlessCI",
    "lint": "ng lint",
    "predeploy": "ng build --configuration production --aot --base-href=/pokedex-angular/",
    "deploy": "gh-pages -d dist/pokedex-angular",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.1.1",
